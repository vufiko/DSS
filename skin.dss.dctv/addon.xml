    <import addon="xbmc.gui" version="5.3.0"/>
	<import addon="repository.dss" version="1.0.1"/>
  </requires>
  <extension
    point="xbmc.gui.skin"
    defaultthemename="textures.xbt"
    debugging="false"
    effectslowdown="0.75">
